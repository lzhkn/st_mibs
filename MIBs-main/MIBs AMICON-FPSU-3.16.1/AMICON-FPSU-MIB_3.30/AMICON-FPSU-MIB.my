  aFpsuHotReserveStateLocalRole OBJECT-TYPE
     SYNTAX   INTEGER {
        unknown          (0),
        notanswer        (1),
        stop             (2),
        error            (3),
        starting         (4),
        working          (5),
        reserving        (6),
        takework         (7),
        ending           (8),
        synchro          (9),
        blocked          (10),
        restart          (11)
     }
     MAX-ACCESS   read-only
     STATUS   current
     DESCRIPTION
       "Роль месного ФПСУ
        starting           стартует
        working            в работе
        reserving          в резерве
        takework           просит работу
        ending             выключается
        synchro            принять настройки и актуальное ПО с другой машины
        blocked            принудительная блокировка (адмом на время смены cfg)
        restart            рестартует (пришел update)"
     ::= { aFpsuHotReserveState 2 }

  aFpsuHotReserveStateRemoteRole OBJECT-TYPE
     SYNTAX   INTEGER {
        unknown          (0),
        notanswer        (1),
        stop             (2),
        error            (3),
        starting         (4),
        working          (5),
        reserving        (6),
        takework         (7),
        ending           (8),
        synchro          (9),
        blocked          (10),
        restart          (11)
     }
     MAX-ACCESS   read-only
     STATUS   current
     DESCRIPTION
       "Роль удаленного ФПСУ
        unknown            неизвестно
        notanswer          не отвечает
        stop               остановлен
        error              ошибка VPN
        starting           стартует
        working            в работе
        reserving          в резерве
        takework           просит работу
        ending             выключается
        synchro            принять настройки и актуальное ПО с другой машины
        blocked            принудительная блокировка (адмом на время смены cfg)
        restart            рестартует (пришл update)"
     ::= { aFpsuHotReserveState 3 }

